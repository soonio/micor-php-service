{
  "main": "serve.php",
  "finder": [
    {
      "notName": "/LICENSE|.*\\.md|.*\\.dist|composer\\..*|.*\\.xml|.*example.*|README\\.*|CHANGELOG\\.*/",
      "exclude": [
        "demo",
        "test",
        "tests",
        "Tests",
        "vendor-bin"
      ],
      "in": "vendor"
    },
    {
      "name": "composer.json",
      "in": "."
    }
  ],
  "git-tag": "version",
  "git-commit-short": "git_commit_short",
  "datetime": "build_datetime",
  "datetime-format": "Y-m-d H:i:s"
}
